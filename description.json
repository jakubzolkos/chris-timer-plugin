{
  "name": "pl-timer",
  "dock_image": "ghcr.io/jakubzolkos/chris-timer-plugin:latest",
  "public_repo": "https://github.com/chris/pl-timer",
  "type": "ds",
  "parameters": [
    {
      "name": "word",
      "type": "str",
      "optional": false,
      "flag": "--word",
      "short_flag": "-w",
      "action": "store",
      "help": "word to count",
      "default": null,
      "ui_exposed": true
    },
    {
      "name": "pattern",
      "type": "str",
      "optional": true,
      "flag": "--pattern",
      "short_flag": "-p",
      "action": "store",
      "help": "input file filter glob",
      "default": "**/*.txt",
      "ui_exposed": true
    }
  ],
  "icon": "",
  "authors": "FNNDSC <dev@babyMRI.org>",
  "title": "ChRIS Timer Plugin",
  "category": "",
  "description": "A ChRIS plugin that outputs the execution time of the main script.",
  "documentation": "https://github.com/jakubzolkos/chris-timer-plugin",
  "license": "MIT",
  "version": "1.0.0",
  "selfpath": "/home/hyron/.local/bin",
  "selfexec": "commandname",
  "execshell": "/usr/bin/python3",
  "min_number_of_workers": 1,
  "max_number_of_workers": 1,
  "min_memory_limit": "100Mi",
  "max_memory_limit": "",
  "min_cpu_limit": "1000m",
  "max_cpu_limit": "",
  "min_gpu_limit": 0,
  "max_gpu_limit": 0
}
